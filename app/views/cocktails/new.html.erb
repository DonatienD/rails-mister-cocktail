<div class="container">
  <div class="page-title-box">
    <h1>New cocktail</h1>
  </div>
  <div class="form-box">
    <%= simple_form_for(@cocktail) do |f| %>
      <%= f.input :name %>
      <%= f.input :photo, as: :file %>
      <%= f.button :submit, class: "btn btn-info add-btn" %>
    <% end %>
  </div>
</div>
